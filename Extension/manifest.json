{
  "manifest_version": 3,
  "name": "Duolingo Black Theme",
  "version": "0.0.1",
  "description": "No desc.",
  "icons": {
    "128": "Images/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.duolingo.com/*"],
      "css": ["style.css"],
      "js" : ["duolingo.js"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_title": "Duolingo themes",  
    "default_popup": "Popup.html"  
  }
}